funcs-wah(){
	cd "$srcdir"
	if type prepare > /dev/null 2>&1; then
		cd "$srcdir"
		echo "Executing \$prepare()"
		prepare
	fi
	if type build > /dev/null 2>&1; then
		cd "$srcdir"
		echo "Executing \$build()"
		build
	fi
	if type check > /dev/null 2>&1; then
		cd "$srcdir"
		echo "Executing \$check()"
		check
	fi
	if type package > /dev/null 2>&1; then
		cd "$srcdir"
		echo "Executing \$package()"
		package
	fi
}
